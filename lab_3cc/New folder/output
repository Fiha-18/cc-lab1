int -> TYPE
n -> ID
, -> COMMA
fact -> ID
= -> ASSIGN
1 -> NUM
; -> SEMI
for -> FOR
( -> LP
int -> TYPE
i -> ID
= -> ASSIGN
1 -> NUM
; -> SEMI
i -> ID
<= -> LTE
n -> ID
; -> SEMI
++ -> INCREMENT
i -> ID
) -> RP
fact -> ID
= -> ASSIGN
fact -> ID
* -> MUL
i -> ID
; -> SEMI
return -> RETURN
fact -> ID
; -> SEMI
